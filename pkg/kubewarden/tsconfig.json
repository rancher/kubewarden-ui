{
  "compilerOptions": {
    "allowJs": true,
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": ".",
    "preserveSymlinks": true,
    "typeRoots": [
      "../../node_modules",
      "../../node_modules/@rancher/shell/types",
      "./core",
      "./types"
    ],
    "types": [
      "node",
      "webpack-env",
      "@types/lodash",
      "rancher",
      "shell",
      "core"
    ],
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "paths": {
      "@shell/core/*": [
        "../../node_modules/@rancher/shell/core/*"
      ],
      "@shell/config/*": [
        "../../node_modules/@rancher/shell/config/*"
      ],
      "@shell/store/*": [
        "../../node_modules/@rancher/shell/store/*"
      ],
      "@shell/components/*": [
        "../../node_modules/@rancher/shell/components/*"
      ],
      "@components/*": [
        "./node_modules/@rancher/components/*"
      ],
      "@/*": [
        "../../*"
      ],
      "@kubewarden/*": [
        "./*"
      ]
    },
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.vue",
    "../../node_modules/@rancher/shell/core/types.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}