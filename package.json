  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@jest/globals": "^29.4.3",
    "@jest/types": "^29.4.3",
    "@types/node": "18.11.9",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/server-test-utils": "1",
    "@vue/test-utils": "1",
    "@vue/vue2-jest": "^28.0.1",
    "babel-jest": "^28.0.0-alpha.1",
    "jest": "^28.0.0-alpha.1",
    "jest-environment-jsdom": "^29.4.3",
    "sass-loader": "10.1.1",
    "ts-jest": "^29.0.5",
